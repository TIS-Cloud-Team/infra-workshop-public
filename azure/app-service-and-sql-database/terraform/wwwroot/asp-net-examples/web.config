<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <connectionStrings>
    <add name="SqlConnectionString" connectionString="Server=tcp:azure-sql-server-name.database.windows.net,1433;Initial Catalog=db-name;Persist Security Info=False;User ID=sql-admin-user-name;Password=sq-admin-password;MultipleActiveResultSets=False;Encrypt=True;TrustServerCertificate=False;Connection Timeout=30;" />
  </connectionStrings>
  <system.web>
    <customErrors mode="Off" />
    <compilation debug="true" targetFramework="4.7.2" />
    <httpRuntime targetFramework="4.7.2" />
  </system.web>
</configuration>